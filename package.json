{
  "name": "fgsnotes",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --dir",
    "build:mac-intel": "electron-builder --mac --x64",
    "build:win32": "electron-builder --win --ia32",
    "build:all": "electron-builder --mac --x64 --win --ia32"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "FrogeeSoft",
    "productName": "FgSNotes",
    "mac": {
      "icon": "src/icon.png",
      "target": [{
        "target": "dmg",
        "arch": ["x64"]
      }],
      "identity": null
    },
    "win": {
      "icon": "src/icon.png",
      "target": ["nsis", "portable"]
    },
    "linux": {
      "icon": "src/icon.png"
    }
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  }
}
